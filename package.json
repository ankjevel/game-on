{
  "name": "game-on",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/ts-node src/index.ts",
    "start:compiled": "node -r source-map-support/register src",
    "dev": "node_modules/.bin/ts-node-dev --no-notify src/index.ts",
    "build": "node_modules/.bin/tsc",
    "lint": "node_modules/.bin/eslint '{types,src}/{**/,}*.ts'",
    "prettier": "node_modules/.bin/prettier --check '{types,src}/{**/,}*.ts'",
    "test": "node_modules/.bin/jest --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "@iteam/config": "12.1.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "ioredis": "4.14.0",
    "jsonwebtoken": "8.5.1",
    "source-map-support": "0.5.13",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@iteam/eslint-config-iteam-node": "0.1.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.1",
    "@types/express-jwt": "0.0.42",
    "@types/ioredis": "4.0.15",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "8.3.3",
    "@types/node": "12.7.3",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "eslint": "6.3.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "jest-expect-message": "1.0.2",
    "jest-watch-typeahead": "0.4.0",
    "mockdate": "2.0.5",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.42",
    "typescript": "3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint && npm run prettier",
      "pre-push": "npm test && npm run lint && npm run prettiert"
    }
  }
}
